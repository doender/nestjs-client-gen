{
    "version": "0.1.2",
    "license": "MIT",
    "main": "dist/cli.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "bin": {
        "nestjs-client-gen": "./cli.js"
    },
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 140,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "name": "nestjs-client-gen",
    "author": "Joost van Doremalen",
    "module": "dist/nestjs-client-gen.esm.js",
    "size-limit": [
        {
            "path": "dist/nestjs-client-gen.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/nestjs-client-gen.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.3",
        "@types/yargs": "^17.0.7",
        "husky": "^7.0.4",
        "size-limit": "^7.0.3",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "code-block-writer": "^11.0.0",
        "ora": "^4.0.2",
        "ts-morph": "^13.0.2",
        "yargs": "^17.3.0"
    }
}
